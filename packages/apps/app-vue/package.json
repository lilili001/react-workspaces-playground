{
  "name": "vue-form",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "core-js": "^2.6.5",
    "hljs": "^6.2.3",
    "huyukundev": "^1.1.1",
    "iview": "^3.5.3",
    "lodash": "^4.17.15",
    "lrz": "^4.9.40",
    "mockjs": "^1.1.0",
    "view-design-miya": "^4.0.3",
    "view-design_huyukundev": "^1.0.0",
    "vue": "^2.6.10",
    "vue-highlightjs": "^1.3.3",
    "vue-hljs": "^1.1.2",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vux": "^2.9.2",
    "yxfeui": "^1.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "compression-webpack-plugin": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "vue-cli-plugin-axios": "^0.0.4",
    "vue-cli-plugin-vux": "^3.0.0",
    "vue-loader": "14.2.3",
    "vue-template-compiler": "^2.6.10",
    "vux-loader": "^1.2.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "no-unused-vars": 0,
      "eqeqeq": "off",
      "camelcase": 0,
      "no-unused-expressions": 0,
      "no-undef": 0,
      "standard/array-bracket-even-spacing": 0,
      "vue/valid-v-on": 0,
      "vue/no-parsing-error": 0,
      "vue/no-use-v-if-with-v-for": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
